@css{
    @font-face {
        font-family: 'slide';
        src:url('fonts/slide.eot');
    }


    @font-face {
        font-family: 'slide';
        src: url(data:application/x-font-woff;charset=utf-8;base64,) format('woff'),
           url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }
}

[data-icon]:before
    font-family: 'slide'
    content: attr(data-icon)
    speak: none
    font-weight: normal
    font-variant: normal
    text-transform: none
    line-height: 1
    -webkit-font-smoothing: antialiased

.icon-IE, .icon-opera, .icon-github, .icon-twitter, .icon-circles, .icon-firefox, .icon-chrome, .icon-safari, .icon-mobile
    font-family: 'slide'
    speak: none
    font-style: normal
    font-weight: normal
    font-variant: normal
    text-transform: none
    line-height: 1
    -webkit-font-smoothing: antialiased

.icon-IE
    color #87d1fb
    &:before
        content: "\e003"

.icon-opera
    color #E78E8E
    &:before
        content: "\e004"

.icon-github:before
    content: "\e006"

.icon-twitter:before
    content: "\e007"

.icon-circles:before
    content: "\e008"

.icon-firefox
    color #fac6a4
    &:before
        content: "\e005"

.icon-chrome
    color #cbefb7
    &:before
        content: "\e001"

.icon-safari
    color #77d8ef
    &:before
        content: "\e002"

.icon-mobile:before
    content: "\e000"


